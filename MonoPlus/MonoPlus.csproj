<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <EnableDefaultItems>false</EnableDefaultItems>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <Title>Mono Plus</Title>
    <Authors>Dzhake</Authors>
    <Company>Dzhake</Company>
    <Description>My own library for MonoGame.</Description>
    <Copyright>MIT License</Copyright>
    <RepositoryUrl>https://github.com/Dzhake/MonoPlus</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>monogame</PackageTags>
    <PackageIcon>Logo.png</PackageIcon>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\Logo.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="MonoGame.Framework.DesktopGL" Version="3.8.2.1105" />
    <PackageReference Include="MonoGame.Content.Builder.Task" Version="3.8.2.1105" />
    <ProjectReference Include="..\SDL3-CS\SDL3\SDL3.Core.csproj" />
  </ItemGroup>
</Project>